{
  "name": "global-nav",
  "version": "2.0.1",
  "description": "A script and stylesheet that displays the Canonical global nav across the top of a site",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "lint": "yarn sass-lint src/**/*.scss -v && yarn eslint src/js",
    "watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubuntudesign/global-nav.git"
  },
  "author": "Anthony Dillon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ubuntudesign/global-nav/issues"
  },
  "homepage": "https://github.com/ubuntudesign/global-nav#readme",
  "devDependencies": {
    "autoprefixer": "9.4.2",
    "babel-core": "6.26.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "cssnano": "4.1.7",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.14.0",
    "husky": "1.3.1",
    "postcss": "7.0.6",
    "prettier": "1.16.3",
    "pretty-quick": "1.10.0",
    "rollup": "0.63.5",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-sass": "0.9.3",
    "rollup-plugin-uglify": "4.0.0",
    "sass-lint": "1.12.1",
    "vanilla-framework": "1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
